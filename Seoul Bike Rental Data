        "from datetime import datetime\n",
        "date_string = '%d/%m/%y'\n",
        "def convert_date_format(date_string):\n",
        "  try:\n",
        "    datetime_object = datetime.strptime(date_string, '%d/%m/%y')\n",
        "    return datetime_object.strftime('%m/%d/%Y')\n",
        "  except ValueError:\n",
        "    return date_string\n",
